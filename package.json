{
  "name": "kaabatrip",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sh -c 'PIDS=$(lsof -tiTCP:3000 -sTCP:LISTEN 2>/dev/null); [ -n \"$PIDS\" ] && kill -9 $PIDS' 2>/dev/null; next dev --hostname 127.0.0.1 --port 3000",
    "dev:clean": "sh -c 'PIDS=$(lsof -tiTCP:3000 -sTCP:LISTEN 2>/dev/null); [ -n \"$PIDS\" ] && kill -9 $PIDS; rm -rf .next' && npm run dev",
    "dev:reset": "sh -c 'PIDS=$(lsof -tiTCP:3000 -sTCP:LISTEN 2>/dev/null); [ -n \"$PIDS\" ] && kill -9 $PIDS' 2>/dev/null; rimraf .next node_modules/.cache && npm run dev",
    "dev:turbo": "rimraf .next && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --reporter=verbose",
    "e2e": "playwright test",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.13",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
